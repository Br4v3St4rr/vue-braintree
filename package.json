{
  "name": "vue-braintree",
  "version": "0.0.4",
  "description": "Vue Braintree Component",
  "main": "dist/index.js",
  "engines": {
    "node": ">=7.0.0"
  },
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "test": "mocha-webpack --webpack-config webpack-test.config.js --require test/setup.js test/**/*.spec.js",
    "test-watch": "mocha-webpack --webpack-config webpack-test.config.js --watch --require test/setup.js test/**/*.spec.js",
    "cover": "cross-env NODE_ENV=coverage nyc --mode development --reporter=lcov --reporter=text npm run test",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/francoislevesque/vue-braintree.git"
  },
  "author": "François Lévesque",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/francoislevesque/vue-braintree/issues"
  },
  "homepage": "https://github.com/francoislevesque/vue-braintree#readme",
  "dependencies": {
    "axios": "0.*",
    "braintree-web-drop-in": "^1.10.0",
    "vue": "2.*"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.16",
    "babel-loader": "^7.1.4",
    "cross-env": "^5.1.5",
    "expect": "^22.4.3",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jsdom": "^11.10.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^5.1.1",
    "mocha-webpack": "^2.0.0-beta.0",
    "nyc": "^11.7.3",
    "vue-loader": "^15.0.10",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.8.2",
    "webpack-bundle-analyzer": "^2.11.2",
    "webpack-cli": "^2.1.3"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "instrument": false,
    "sourceMap": false,
    "extension": [
      ".vue"
    ]
  }
}
